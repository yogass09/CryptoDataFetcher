    {
      "cell_type": "code",
      "source": [
        "library(crypto2)\n",
        "library(dplyr)\n",
        "\n",
        "# Define the parameters\n",
        "start_date <- \"20130101\"\n",
        "end_date <- \"20240101\"\n",
        "convert_currency <- \"USD\"\n",
        "\n",
        "# Retrieve the list of all coins to find the IDs for BTC and ETH\n",
        "coins_list <- crypto_list()\n",
        "\n",
        "# Filter to find BTC and ETH in the coins list\n",
        "btc_eth_list <- coins_list %>%\n",
        "  filter(symbol %in% c(\"BTC\", \"ETH\"))\n",
        "\n",
        "# Assuming we now have the correct format or IDs for BTC and ETH, use them to fetch historical data\n",
        "btc_data <- crypto_history(\n",
        "  coin_list = filter(btc_eth_list, symbol == \"BTC\"),\n",
        "  convert = convert_currency,\n",
        "  start_date = start_date,\n",
        "  end_date = end_date,\n",
        "  single_id = TRUE # If fetching data for a single coin at a time\n",
        ")\n",
        "\n",
        "\n",
        "eth_data <- crypto_history(\n",
        "  coin_list = filter(btc_eth_list, symbol == \"ETH\"),\n",
        "  convert = convert_currency,\n",
        "  start_date = start_date,\n",
        "  end_date = end_date,\n",
        "  single_id = TRUE # If fetching data for a single coin at a time\n",
        ")\n",
        "\n",
        "\n"
      ],
