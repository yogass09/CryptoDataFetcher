        "# Calculate the percentage change in the 'close' price\n",
        "eth_data_pct_change <- eth_data_close %>%\n",
        "  arrange(timestamp) %>%\n",
        "   mutate(eth_pct_change = round((close - lag(close)) / lag(close) * 100, digits = 2))\n",
        "\n",
        "eth_data_pct_change %>%\n",
        "  filter(!is.na(eth_pct_change))\n"
