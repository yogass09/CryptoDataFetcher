        "library(crypto2)\n",
        "library(dplyr)\n",
        "\n",
        "# Assuming btc_data and eth_data have been fetched\n",
        "# Directly convert timestamp to Date without specifying the format\n",
        "btc_data <- btc_data %>%\n",
        "  mutate(timestamp = as.Date(as.character(timestamp)))\n",
        "\n",
        "eth_data <- eth_data %>%\n",
        "  mutate(timestamp = as.Date(as.character(timestamp)))\n",
        "\n",
        "# Check the first few rows to ensure the timestamp has been correctly converted\n",
        "head(btc_data)\n",
        "head(eth_data)\n"
