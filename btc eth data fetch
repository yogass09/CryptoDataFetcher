      "source": [
        "# Calculate the percentage change in the 'close' price\n",
        "btc_data_pct_change <- btc_data_close %>%\n",
        "  arrange(timestamp) %>%\n",
        "   mutate(btc_pct_change = round((close - lag(close)) / lag(close) * 100, digits = 2))\n",
        "\n",
        "btc_data_pct_change %>%\n",
        "  filter(!is.na(btc_pct_change))\n"
