        "library(ggplot2)\n",
        "library(dplyr)\n",
        "library(tidyr)\n",
        "library(lubridate)\n",
        "\n",
        "# Function to create plot for a given period\n",
        "create_period_plot <- function(data, start_year, end_year) {\n",
        "  period_data <- data %>%\n",
        "    filter(year(timestamp) >= start_year & year(timestamp) <= end_year)\n",
        "\n",
        "  plot <- ggplot(period_data, aes(x = timestamp, y = Value, color = Metric)) +\n",
        "    geom_line() +\n",
        "    labs(title = paste(\"Cryptocurrency Metrics\", start_year, \"to\", end_year),\n",
        "         x = \"Timestamp\",\n",
        "         y = \"Metric Value\") +\n",
        "    theme_minimal() +\n",
        "    theme(legend.title = element_blank(), aspect.ratio = 9 / 16)\n",
        "\n",
        "  return(plot)\n",
        "}\n",
        "\n",
        "# Assuming merged_data_long is already created\n",
        "\n",
        "# Create plots for each specified period\n",
        "plot_2015_2018 <- create_period_plot(merged_data_long, 2015, 2018)\n",
        "plot_2019_2021 <- create_period_plot(merged_data_long, 2019, 2021)\n",
        "plot_2022_2024 <- create_period_plot(merged_data_long, 2022, 2024)\n",
        "\n"
      ],
